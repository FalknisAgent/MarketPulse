{
    "version": 2,
    "buildCommand": "cd api && npm install && cd ../client && npm install && npm run build",
    "outputDirectory": "client/dist",
    "framework": null,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}